{
  "name": "webclient",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ssr": "ng run WebClient:server:dev",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-locale:en": "ng build --prod --i18n-locale en --i18n-format xlf --i18n-file src/locale/messages.en.xlf --output-path=dist/en --baseHref /en/",
    "build-locale:bg": "ng build --prod --i18n-locale bg --i18n-format xlf --i18n-file src/locale/messages.bg.xlf --output-path=dist/bg --baseHref /bg/",
    "build-locale": "npm run build-locale:en && npm run build-locale:bg"
  },
  "private": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^11.1.0",
    "@angular/animations": "12.0.0-next.2",
    "@angular/cdk": "~11.2.2",
    "@angular/common": "12.0.0-next.2",
    "@angular/compiler": "12.0.0-next.2",
    "@angular/core": "12.0.0-next.2",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "12.0.0-next.2",
    "@angular/localize": "^12.0.0-next.2",
    "@angular/material": "^11.2.2",
    "@angular/platform-browser": "12.0.0-next.2",
    "@angular/platform-browser-dynamic": "12.0.0-next.2",
    "@angular/platform-server": "12.0.0-next.2",
    "@angular/router": "12.0.0-next.2",
    "@ckeditor/ckeditor5-angular": "^2.0.1",
    "@ckeditor/ckeditor5-build-balloon": "^27.1.0",
    "@ckeditor/ckeditor5-build-classic": "^27.1.0",
    "@ckeditor/ckeditor5-theme-lark": "^27.1.0",
    "@ng-bootstrap/ng-bootstrap": "^5.3.1",
    "@nguniversal/module-map-ngfactory-loader": "8.1.1",
    "aspnet-prerendering": "^3.0.1",
    "bootstrap": "^4.6.0",
    "core-js": "^3.9.0",
    "jquery": "^3.5.1",
    "moment": "^2.29.1",
    "oidc-client": "^1.11.5",
    "popper.js": "^1.16.0",
    "rxjs": "^6.6.6",
    "sweetalert2": "^10.15.5",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.2",
    "@angular/cli": "^11.2.2",
    "@angular/compiler-cli": "^12.0.0-next.2",
    "@angular/language-service": "^12.0.0-next.2",
    "@types/jasmine": "~3.4.4",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "~12.11.6",
    "codelyzer": "^5.2.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "typescript": "4.1.5"
  },
  "optionalDependencies": {
    "node-sass": "^5.0.0",
    "protractor": "~5.4.2",
    "ts-node": "~8.4.1",
    "tslint": "~5.20.0"
  }
